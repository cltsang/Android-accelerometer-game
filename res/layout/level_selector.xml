<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
		android:orientation="vertical"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android"
		android:weightSum="1">
	<LinearLayout
			android:orientation="horizontal"
			android:layout_width="fill_parent"
			android:layout_height="fill_parent" android:weightSum="1"
			android:layout_weight="0.25">
		<ImageButton android:layout_width="fill_parent"	android:layout_height="fill_parent"
					 android:id="@+id/imageButton1" android:layout_weight="0.2" android:contentDescription="1"/>
		<ImageButton android:layout_width="fill_parent" android:layout_height="fill_parent"
					 android:id="@+id/imageButton2" android:layout_weight="0.2" android:contentDescription="2"/>
		<ImageButton android:layout_width="fill_parent" android:layout_height="fill_parent"
					 android:id="@+id/imageButton3" android:layout_weight="0.2" android:contentDescription="3"/>
		<ImageButton android:layout_width="fill_parent" android:layout_height="fill_parent"
					 android:id="@+id/imageButton4" android:layout_weight="0.2" android:contentDescription="4"/>
		<ImageButton android:layout_width="fill_parent" android:layout_height="fill_parent"
					 android:id="@+id/imageButton5" android:layout_weight="0.2" android:contentDescription="5"/>
	</LinearLayout>
	<LinearLayout
			android:orientation="horizontal"
			android:layout_width="fill_parent"
			android:layout_height="fill_parent" android:weightSum="1" android:layout_weight="0.25">
		<ImageButton android:layout_width="fill_parent" android:layout_height="fill_parent"
					 android:id="@+id/imageButton6" android:layout_weight="0.2" android:contentDescription="6"/>
		<ImageButton android:layout_width="fill_parent" android:layout_height="fill_parent"
					 android:id="@+id/imageButton7" android:layout_weight="0.2" android:contentDescription="7"/>
		<ImageButton android:layout_width="fill_parent" android:layout_height="fill_parent"
					 android:id="@+id/imageButton8" android:layout_weight="0.2" android:contentDescription="8"/>
		<ImageButton android:layout_width="fill_parent" android:layout_height="fill_parent"
					 android:id="@+id/imageButton9" android:layout_weight="0.2" android:contentDescription="9"/>
		<ImageButton android:layout_width="fill_parent" android:layout_height="fill_parent"
					 android:id="@+id/imageButton10" android:layout_weight="0.2" android:contentDescription="10"/>
	</LinearLayout>
	<LinearLayout
			android:orientation="horizontal"
			android:layout_width="fill_parent"
			android:layout_height="fill_parent" android:weightSum="1" android:layout_weight="0.25">
		<ImageButton android:layout_width="fill_parent" android:layout_height="fill_parent"
					 android:id="@+id/imageButton11" android:layout_weight="0.2" android:contentDescription="11"/>
		<ImageButton android:layout_width="fill_parent" android:layout_height="fill_parent"
					 android:id="@+id/imageButton12" android:layout_weight="0.2" android:contentDescription="12"/>
		<ImageButton android:layout_width="fill_parent" android:layout_height="fill_parent"
					 android:id="@+id/imageButton13" android:layout_weight="0.2" android:contentDescription="13"/>
		<ImageButton android:layout_width="fill_parent" android:layout_height="fill_parent"
					 android:id="@+id/imageButton14" android:layout_weight="0.2" android:contentDescription="14"/>
		<ImageButton android:layout_width="fill_parent" android:layout_height="fill_parent"
					 android:id="@+id/imageButton15" android:layout_weight="0.2" android:contentDescription="15"/>
	</LinearLayout>
	<LinearLayout
			android:orientation="horizontal"
			android:layout_width="fill_parent"
			android:layout_height="fill_parent" android:weightSum="1" android:layout_weight="0.25">
		<ImageButton android:layout_width="fill_parent" android:layout_height="fill_parent"
					 android:id="@+id/imageButton16" android:layout_weight="0.2" android:contentDescription="16"/>
		<ImageButton android:layout_width="fill_parent" android:layout_height="fill_parent"
					 android:id="@+id/imageButton17" android:layout_weight="0.2" android:contentDescription="17"/>
		<ImageButton android:layout_width="fill_parent" android:layout_height="fill_parent"
					 android:id="@+id/imageButton18" android:layout_weight="0.2" android:contentDescription="18"/>
		<ImageButton android:layout_width="fill_parent" android:layout_height="fill_parent"
					 android:id="@+id/imageButton19" android:layout_weight="0.2" android:contentDescription="19"/>
		<ImageButton android:layout_width="fill_parent" android:layout_height="fill_parent"
					 android:id="@+id/imageButton20" android:layout_weight="0.2" android:contentDescription="20"/>
	</LinearLayout>
</LinearLayout>
